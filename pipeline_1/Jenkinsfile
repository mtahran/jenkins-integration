pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Mustafa') {
            steps {
                echo 'Mustafa Tahran'
            }
        }
        stage('Illinois') {
            steps {
                echo 'Illinois'
            }
        }
        stage('passwd') {
            steps {
                sh 'cat /etc/passwd'
            }
        }
    }
}